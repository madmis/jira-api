{
    "name": "madmis/jira-api",
    "description": "Jira REST API php client",
    "license": "MIT",
    "keywords": ["jira","rest","api"],
    "type": "library",
    "authors": [
        {
            "name": "Machin Dmitry",
            "homepage": "http://madmis.com.ua/",
            "email": "machin.dmitry@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/psr7": "^1.2",
        "guzzlehttp/guzzle": "^6.2",
        "httplib/http": "^1.0",
        "jms/serializer": "^1.0",
        "symfony/options-resolver": "^2.7|^3.1"
    },
    "autoload": {
        "psr-4" : {
            "madmis\\" : "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "behat/behat": "^3.0",
        "symfony/console": "^2.7|^3.1"
    }
}
